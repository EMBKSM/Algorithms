#include <bits/stdc++.h>

using namespace std;

int main() {
	int n;
	
	cin >> n;
	for (int i = 0; i < n; i++) {
		string ox_list;
		int score = 0, cnt = 0;
		cin >> ox_list;

		for (int j = 0; j < ox_list.length(); j++) {
			if (ox_list[j] == 'X') {
				cnt = 0;
			}
			else {
				cnt++;
				score += cnt;
				
			}
		}
		cout << score << "\n";
	}
}