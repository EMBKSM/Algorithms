#include <bits/stdc++.h>

using namespace std;

int main() {
	int n;
	
	cin >> n;
	for (int i = 0; i < n; i++) {
		int m;
		string re_text;

		cin >> m >> re_text;
		for (int j = 0; j < re_text.length(); j++) {
			for (int k = 0; k < m; k++) {
				cout << re_text[j];
			}
		}
		cout << "\n";
	}
}